generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id         String   @id @default(uuid())
  name       String
  mail       String   @unique
  verified   Boolean  @default(false)
  password   String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt()
}
